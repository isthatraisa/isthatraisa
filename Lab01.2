#include <iostream>

using namespace std;

int main()
{
    int r=3,c=3;
    int** arr2D=new int*[r];
    for(int i=0;i<r;i++)
    {
        arr2D[i]=new int[r];
    }
    cout<<"Enter array element: "<<endl;
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            cin>> arr2D[i][j];
        }
    }
    cout<< "Print array elemrnt: "<<endl;
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            cout<< arr2D[i][j]<<" ";
        }
        cout<<endl;
    }
    delete[] arr2D;
    return 0;
}
